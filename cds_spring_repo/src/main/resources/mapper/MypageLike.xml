<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.human.cds.mapper.MypageLikeMapper">
	<!-- id는 dao에서 sql.session뒤에 .뭐시기 그거랑 맞춰야함  -->
	
    <!-- 카테고리로 데이터 필터링 -->
    <select id="getLikeyListByCategory" parameterType="string" resultType="LikeyVO">
        SELECT * FROM likey WHERE category = #{category}
    </select>
	


	
</mapper>



