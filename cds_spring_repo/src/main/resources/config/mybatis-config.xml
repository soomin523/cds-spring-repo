		<typeAlias alias="DestinationDBVO" type="com.human.cds.vo.DestinationDBVO"/>
		<typeAlias alias="AcommoImgVO" type="com.human.cds.vo.AcommoImgVO"/>
		<typeAlias alias="AccommodationRoomVO" type="com.human.cds.vo.AccommodationRoomVO"/>
